# =============================================================================
# ArBot - Oracle Cloud Environment Variables
# Copy to .env and fill in values: cp .env.example .env
# =============================================================================

# --- Database (install.sh 에서 사용) ---
POSTGRES_PASSWORD=CHANGE_ME_pg_strong_password
REDIS_PASSWORD=CHANGE_ME_redis_strong_password

# --- ArBot Config (위와 동일한 값 입력) ---
ARBOT_DATABASE__POSTGRES__PASSWORD=CHANGE_ME_pg_strong_password
ARBOT_DATABASE__REDIS__PASSWORD=CHANGE_ME_redis_strong_password

# --- Exchange API Keys ---
ARBOT_BINANCE_API_KEY=
ARBOT_BINANCE_API_SECRET=

ARBOT_OKX_API_KEY=
ARBOT_OKX_API_SECRET=
ARBOT_OKX_PASSPHRASE=

ARBOT_UPBIT_API_KEY=
ARBOT_UPBIT_API_SECRET=

# --- Alerts ---
ARBOT_ALERTS__TELEGRAM__BOT_TOKEN=
ARBOT_ALERTS__TELEGRAM__CHAT_ID=
